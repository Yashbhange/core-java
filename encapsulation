 class Student {
    private String name;

    public String setname(String name) {
        this.name = name;
        return name ;

    }
    public String getname() {
        return name;
    }
}

public class encapsulation1 {
    public static void main(String[] args) {
        Student s = new Student();
        s.setname("yash");
        System.out.println(s.getname());

    }
}
